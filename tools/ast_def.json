{
"Expr": [
	"Binary   : Rc<Expr> left, Token operator, Rc<Expr> right",
	"Block    : Rc<Expr> expression",
	"Grouping : Rc<Expr> expression",
	"Literal  : Object value",
	"Logic    : Rc<Expr> left, Token operator, Rc<Expr> right",
	"Unary    : Token operator, Rc<Expr> right",
	"Variable : String name"
],
"Stmt": [
	"AssignStmt  : Rc<Expr> variable, Rc<Expr> value",
	"BlockStmt   : Rc<Vec<Rc<Stmt>>> stmts",
	"ExprStmt    : Rc<Expr> expr",
	"IfStmt      : Rc<Vec<(Rc<Expr>-Rc<Stmt>)>> conditionals, Rc<Option<Rc<Stmt>>> else_block",
	"PrintStmt   : Rc<Expr> expr",
	"VarDeclStmt : Rc<Expr> variable, Rc<Expr> value",
	"WhileStmt   : Rc<Expr> condition, Rc<Stmt> block"
]
}
